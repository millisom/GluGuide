    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^2.1.4",
    "babel-jest": "^29.7.0",
    "chromedriver": "^130.0.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
